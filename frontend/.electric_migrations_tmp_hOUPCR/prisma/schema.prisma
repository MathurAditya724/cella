generator client {
  provider = "prisma-client-js"
  output   = "/home/gino/Github/cella/frontend/src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://prisma:proxy_password@0.0.0.0:65432/electric?sslmode=disable"
}

model Labels {
  id         String  @id @db.VarChar
  name       String  @db.VarChar
  color      String? @db.VarChar
  project_id String  @db.VarChar
  @@map("labels")
}

model Tasks {
  id          String    @id @db.VarChar
  slug        String    @db.VarChar
  markdown    String?   @db.VarChar
  summary     String    @db.VarChar
  type        String    @db.VarChar
  impact      Int? /// @zod.number.int().gte(-2147483648).lte(2147483647)
  sort_order  Int? /// @zod.number.int().gte(-2147483648).lte(2147483647)
  status      Int /// @zod.number.int().gte(-2147483648).lte(2147483647)
  project_id  String    @db.VarChar
  created_at  DateTime  @db.Timestamp(6)
  created_by  String    @db.VarChar
  assigned_by String?   @db.VarChar
  assigned_at DateTime? @db.Timestamp(6)
  modified_at DateTime? @db.Timestamp(6)
  modified_by String?   @db.VarChar
  @@map("tasks")
}
