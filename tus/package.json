    "config": "workspace:*",
    "dotenv": "^16.4.5",
    "tsx": "^4.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
