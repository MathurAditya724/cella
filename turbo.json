{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV"
  ],
  "globalDependencies": [
    "config",
    ".env"
  ],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:dev": {
      "dependsOn": [
        "^build:dev"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check:types": {},
    "email:dev": {},
    "push": {
      "persistent": true
    },
    "migrate": {},
    "migrate:pg": {},
    "electric:migrate": {},
    "generate": {
      "persistent": true
    },
    "generate:pg": {
      "persistent": true
    },
    "electric:generate": {
      "persistent": true
    },
    "electric:generate:custom": {},
    "studio": {},
    "electric:studio": {},
    "seed": {},
    "seed:pivotal": {},
    "seed:user": {},
    "seed:organizations": {},
    "seed:data": {}
  }
}